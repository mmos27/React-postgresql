generator client {
  provider = "prisma-client-js"
  output   = "../frontend/node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  createdAt DateTime @default(now())
}

model fish_data {
  ID Int @id @default(autoincrement())
  FISHSIZE Float
  FISHSPECIES String
  createdAt DateTime @default(now())
}